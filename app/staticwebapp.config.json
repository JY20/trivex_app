{
  "navigationFallback": {
    "rewrite": "index.html"
  },
  "routes": [
    {
      "route": "/api/*",
      "allowedRoles": ["anonymous"]
    }
  ],
  "platformErrorOverrides": [
    {
      "errorType": "NotFound",
      "serve": "/index.html"
    }
  ],
  "mimeTypes": {
    ".json": "text/json"
  }
}
